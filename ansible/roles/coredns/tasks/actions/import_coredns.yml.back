---
- name: import dns
  etcd3:
    key: "test"
    vaule: "thisistest123"
    state: "preset"
    ca_cert: "{{ coredns.local.ssl_dir }}/ca.pem"
    client_cert: "{{ coredns.local.ssl_dir }}/client.pem"
